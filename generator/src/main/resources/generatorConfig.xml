<!DOCTYPE generatorConfiguration PUBLIC
        "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
    <context id="kotlin" targetRuntime="MyBatis3Kotlin">
        <plugin type="org.mybatis.generator.plugins.MapperAnnotationPlugin"/>
        <plugin type="com.github.dkurata38.mybatis_generator_plugins.kotlin.NotNullFieldPlugin"/>
        <commentGenerator>
            <property name="suppressDate" value="true"/>
            <property name="addRemarkComments" value="false"/>
        </commentGenerator>
        <jdbcConnection driverClass="org.postgresql.Driver"
                        connectionURL="jdbc:postgresql://localhost:5432/open_web_library"
                        userId="postgres" password="postgres"/>

        <javaTypeResolver>
            <property name="useJSR310Types" value="true"/>
        </javaTypeResolver>

        <javaModelGenerator targetPackage="com.github.dkurata38.open_web_library.repository" targetProject="/Users/kuratadaisuke/Projects/open-web-library/source/open-web-library/generator/src/main/kotlin">
            <property name="enableSubPackages" value="true"/>
        </javaModelGenerator>
        <javaClientGenerator targetPackage="com.github.dkurata38.open_web_library.repository" targetProject="/Users/kuratadaisuke/Projects/open-web-library/source/open-web-library/generator/src/main/kotlin">
            <property name="enableSubPackages" value="true"/>
        </javaClientGenerator>

        <table tableName="member"/>
        <table tableName="member_credential"/>
    </context>
</generatorConfiguration>
